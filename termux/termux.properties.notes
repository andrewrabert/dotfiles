soft-keyboard-toggle-behavior = enable
bell-character = ignore
extra-keys = [ \
 [ \
     {key: '|', popup: 'm'}, \
     {key: '/', popup: "BACKSLASH"}, \
     {key: '$'}, \
     {key: '~'}, \
     {key: '_'}, \
     {key: '-'}, \
     {macro: "BACKSLASH c", display: ✓} \
 ], \
 [ \
     {key: 'TAB'}, \
     {key: 'CTRL'}, \
     {key: 'ALT'}, \
     {key: ':'}, \
     {macro: "CTRL d", display: ←}, \
     {macro: "CTRL t", display: →}, \
     {key: 'ESC'} \
 ] \
]
