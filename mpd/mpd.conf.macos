music_directory     "~/.config/mpd/library"
db_file             "~/.config/mpd/mpd.db"
log_file            "~/.config/mpd/mpd.log"
state_file          "~/.config/mpd/mpd.state"
playlist_directory  "~/.config/mpd/playlists"

restore_paused      "yes"

input {
    plugin  "curl"
}

samplerate_converter    "Best Sinc Interpolator"

audio_output {
    type                  "osx"
    name                  "CoreAudio"
    mixer_type            "software"
}
