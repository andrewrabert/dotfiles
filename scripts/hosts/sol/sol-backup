#!/usr/bin/env sh
exec sudo rsync \
    -av \
    --progress \
    --delete \
    --exclude .cache \
    --exclude .local \
    --exclude .npm \
    --exclude .nuget \
    --exclude .rustup \
    --exclude Code/sol-docker/volumes/jellyfin/cache \
    --exclude Code/sol-docker/volumes/jellyfin/config/log \
    --exclude Code/sol-docker/volumes/jellyfin/config/transcoding-temp \
    ~ar /storage/Backup/sol/home
